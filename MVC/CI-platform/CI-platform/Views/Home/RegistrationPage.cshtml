                        <div class="mb-3 fname">
                            <label for="fname" class="form-label mb-0">First Name</label>
                            <input type="text" class="form-control h-lg-100 h-md-75 h-sm-" id="fname" placeholder="evan">
                        </div>
                        <div class="mb-3 lname">
                            <label for="lname" class="form-label mb-0">Last Name</label>
                            <input type="text" class="form-control" id="lname" placeholder="donohue">
                        </div>
                        <div class="mb-3 phone">
                            <label for="phone" class="form-label mb-0">Phone Number</label>
                            <input type="tel" class="form-control" id="phone" placeholder="8923454615">
                        </div>
                        <div class="mb-3 mail">
                            <label for="email" class="form-label mb-0">Email address</label>
                            <input type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="evan.donohue@gmail.com">
                        </div>
                        <div class="mb-3 pass">
                            <label for="password" class="form-label mb-0" >Password</label>                                
                            <input type="password" class="form-control" id="password" placeholder="**********">
                        </div>
                        <div class="mb-3 repass">
                            <label for="repassword" class="form-label mb-0" >Confrim Password</label>                                
                            <input type="password" class="form-control" id="repassword" placeholder="**********">
                        </div>
                        <button type="submit" class="register-btn">Register</button>
                        <div id="forgotpw" class="mt-3 text-center"><a href="forgot.html">Lost your password?</a></div>
                        <div id="login" class="mt-3 text-center">Already registered? <a href="login.html">Login now</a></div>
